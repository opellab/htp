<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:htp="http://purl.org/htp#">

    <!-- This ontology represents the HiTOP model, detailing mental health disorders and their classifications. -->
    <!-- It includes mechanisms for future updates and expansions to accommodate evolving models and research. -->
    <owl:Ontology rdf:about="http://purl.org/htp">
        <rdfs:label>Dynamic Ontology for Mental Health Classification</rdfs:label>
        <rdfs:comment>Dynamic Ontology for Mental Health Classification representing various classes to structure mental health dimensions in the HiTOP model.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://www.hitop-system.org/"/>
        <owl:versionInfo>1.5</owl:versionInfo>
        <dc:creator>
            <foaf:Person>
                <foaf:name>Dr. Rogério Blitz</foaf:name>
                <foaf:mbox rdf:resource="mailto:rogerio.blitz@med.uni-jena.de"/>
            </foaf:Person>
        </dc:creator>
        <dc:date>2024-08-21</dc:date>
        <dc:publisher></dc:publisher>
        <dc:contributor>Dr. Rogério Blitz, Prof. Dr. Nils Opel</dc:contributor>
        <dcterms:issued>2024-08-18</dcterms:issued>
        <dcterms:rightsHolder>Dr. Rogério Blitz, Prof. Dr. Nils Opel</dcterms:rightsHolder>
        <dcterms:rightsHolder>Universitätsklinikum Jena</dcterms:rightsHolder>
        <dcterms:license rdf:resource="https://creativecommons.org/licenses/by-nc-nd/4.0/"/>
        <dcterms:rights>This work is licensed under a Creative Commons Attribution-NonCommercial-NoDerivs 4.0 International License.</dcterms:rights>
    </owl:Ontology>

    <!-- Indicates potential future updates to the taxonomy, allowing for new categories and components. -->
    <!-- Highlights the ontology's flexibility to incorporate new elements and relationships as research evolves. -->
    <owl:AnnotationProperty rdf:about="http://purl.org/htp#futureReleaseInfo">
        <rdfs:label>Future Release Information</rdfs:label>
        <rdfs:comment>
            This ontology is designed with flexibility to accommodate both vertical and horizontal changes, reflecting the evolving nature of taxonomy. 
            It utilizes a dynamic taxonomy that supports the seamless integration of new categories and subcategories, avoiding a rigid subclass hierarchy. 
            The ontology employs properties such as `hasSuperspectra`, `hasSpectra`, `hasSubfactor`, `hasSyndrome`, `hasComponent`, `hasTrait`, `hasSymptom`, 
            and `hasDisorder` to define clear relationships and maintain hierarchical adaptability. Future updates may include adding new categories
            to reflect emerging research findings and evolving scientific insights. This design ensures that the ontology can easily incorporate 
            new taxonomic elements and relationships, thus supporting continuous enhancement and refinement.
        </rdfs:comment>
    </owl:AnnotationProperty>

    <!-- This ontology defines various classes to structure mental health dimensions in the HiTOP model. -->
    <!-- The classes range from broad categories (Superspectra) to specific manifestations (Disorder), detailing the hierarchical structure of mental health attributes. -->
    <owl:Class rdf:about="http://purl.org/htp#Superspectra">
        <rdfs:label>Superspectra</rdfs:label>
        <rdfs:comment>A broad category in the HiTOP model that encompasses large domains of psychological phenomena, often representing overarching patterns of mental health.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Spectra">
        <rdfs:label>Spectra</rdfs:label>
        <rdfs:comment>A level within the HiTOP model that groups related mental health issues into broad categories, providing a framework for understanding complex symptomatology.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Superspectra"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Subfactor">
        <rdfs:label>Subfactor</rdfs:label>
        <rdfs:comment>A more specific grouping within a spectrum, representing clusters of symptoms or traits that share common characteristics.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Spectra"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Syndrome">
        <rdfs:label>Syndrome</rdfs:label>
        <rdfs:comment>A collection of symptoms that often occur together and characterize a particular mental disorder, as identified within the HiTOP framework.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Subfactor"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Component">
        <rdfs:label>Component</rdfs:label>
        <rdfs:comment>A specific aspect of a Syndrome or subfactor, representing distinct elements of psychological functioning or dysfunction.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Syndrome"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Trait">
        <rdfs:label>Trait</rdfs:label>
        <rdfs:comment>A stable characteristic or disposition that influences behavior and thought patterns, considered in the HiTOP model as part of individual differences.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Syndrome"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Symptom">
        <rdfs:label>Symptom</rdfs:label>
        <rdfs:comment>An observable sign or manifestation of a mental disorder, often used to diagnose and classify conditions within the HiTOP framework.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Syndrome"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Component"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Disorder">
        <rdfs:label>Disorder</rdfs:label>
        <rdfs:comment>A clinically recognized condition characterized by a specific set of symptoms and behaviors, categorized in the HiTOP model for diagnosis and treatment planning.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/htp#Subfactor"/>
    </owl:Class>

    <!-- These object properties define the hierarchical and associative relationships between different mental health constructs in the HiTOP model. -->
    <!-- They connect broad categories (Superspectra) down to specific manifestations (Disorder), detailing how each class relates to others. -->
    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasSuperspectra">
        <rdfs:label>has Superspectra</rdfs:label>
        <rdfs:comment>This property associates a concept with its corresponding superspectra, representing broad domains of psychological phenomena.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/htp#Superspectra"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of additional superspectra or modification of existing ones to reflect new developments in the model.
            Supports flexibility in expanding or refining the taxonomy based on emerging research and evolving understanding of psychological phenomena.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasSpectra">
        <rdfs:label>has Spectra</rdfs:label>
        <rdfs:comment>This property links a superspectra to its underlying spectra, reflecting a more specific category within the domain.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/htp#Superspectra"/>
        <rdfs:range rdf:resource="http://purl.org/htp#Spectra"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future addition of new spectra or revision of existing ones to better categorize psychological phenomena.
            Supports adaptability in expanding the taxonomy to include emerging spectra based on new research and evolving theoretical frameworks.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasSubfactor">
        <rdfs:label>has Subfactor</rdfs:label>
        <rdfs:comment>This property connects a spectra to its subfactors, highlighting the distinct elements that make up the spectra.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:range rdf:resource="http://purl.org/htp#Subfactor"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of additional subfactors or modification of existing ones to reflect new insights and finer categorizations.
            Supports flexibility in adapting the taxonomy to include new subfactors or refine existing ones based on ongoing research advancements.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasSyndrome">
        <rdfs:label>has Syndrome</rdfs:label>
        <rdfs:comment>This property associates a concept with its Syndromes, identifying collections of symptoms that occur together.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/htp#Syndrome"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of new syndromes or revision of current ones to better reflect evolving clinical and research findings.
            Supports the ability to update and expand the taxonomy to include new syndromic classifications as our understanding of psychological conditions progresses.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasComponent">
        <rdfs:label>has Component</rdfs:label>
        <rdfs:comment>This property links a Syndrome to its components, detailing the individual parts that form the Syndrome.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/htp#Syndrome"/>
        <rdfs:range rdf:resource="http://purl.org/htp#Component"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future addition of new components or revision of existing ones to capture more detailed aspects of psychological phenomena.
            Supports flexibility in updating and expanding the taxonomy with new components as more granular research findings emerge.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasTrait">
        <rdfs:label>has Trait</rdfs:label>
        <rdfs:comment>This property connects a Syndrome to its traits, highlighting specific characteristics or tendencies within the Syndrome.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/htp#Syndrome"/>
        <rdfs:range rdf:resource="http://purl.org/htp#Trait"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of new traits or modification of existing ones to reflect advancements in personality and psychological research.
            Supports adaptability in refining and expanding the taxonomy to include new traits based on evolving understanding and empirical data.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasSymptom">
        <rdfs:label>has Symptom</rdfs:label>
        <rdfs:comment>This property links a concept to its symptoms, which are observable signs of a condition or disorder.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/htp#Symptom"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of new symptoms or modification of existing ones to better align with clinical and research developments.
            Supports the ability to update and expand the taxonomy with new symptoms as our understanding of psychological disorders evolves.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/htp#hasDisorder">
        <rdfs:label>has Disorder</rdfs:label>
        <rdfs:comment>This property associates a concept with specific disorders, providing a link to clinically recognized mental health conditions.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/htp#Disorder"/>
        <htp:futureReleaseInfo>
            Indicates the potential for future addition of new disorders or revision of existing ones to reflect advances in psychiatric and psychological research.
            Supports the flexibility to expand the taxonomy with new disorder categories and update current classifications as new insights become available.
        </htp:futureReleaseInfo>
    </owl:ObjectProperty>

    <!-- These classes are part of the Superspectra category in the HiTOP model, representing major dimensions of mental health phenomena. -->
    <!-- They include broad categories such as General Psychopathology. -->
    <owl:Class rdf:about="http://purl.org/htp#GeneralPsychopathology">
        <rdfs:label>General Psychopathology</rdfs:label>
        <rdfs:comment>General Psychopathology refers to the broad, overarching category of mental disorders and psychological symptoms that can manifest across different mental health conditions. It encompasses a wide range of psychological phenomena that are not specific to any single disorder but may contribute to or exacerbate multiple types of mental health issues. This category includes general symptoms and traits that are commonly observed in various psychiatric conditions, such as mood disturbances, anxiety, cognitive dysfunctions, and behavioral problems.</rdfs:comment>
    </owl:Class>

    <!-- These classes represent the main dimensions of psychopathological spectra in the HiTOP model, including various patterns of psychological and behavioral symptoms. -->
    <!-- They encompass broad categories like Internalizing and Thought Disorder, highlighting different aspects of mental health. -->
    <owl:Class rdf:about="http://purl.org/htp#Somatoform">
        <rdfs:label>Somatoform</rdfs:label>
        <rdfs:comment>The Somatoform label refers to a category of mental health disorders characterized by physical symptoms that cannot be fully explained by any underlying medical condition, highlighting the complex interaction between mind and body in mental health.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Internalizing">
        <rdfs:label>Internalizing</rdfs:label>
        <rdfs:comment>The Internalizing label refers to a category of mental health disorders characterized by inward-focused symptoms such as anxiety, depression, and withdrawal, reflecting emotional and cognitive distress.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#ThoughtDisorder">
        <rdfs:label>Thought Disorder</rdfs:label>
        <rdfs:comment>Thought Disorder refers to a category of mental health conditions characterized by disorganized thinking, impaired reality testing, and disturbances in the form or content of thought.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Detachment">
        <rdfs:label>Detachment</rdfs:label>
        <rdfs:comment>Detachment refers to a psychological state characterized by emotional distance and disengagement from social interactions.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#DisinhibitedExternalizing">
        <rdfs:label>Disinhibited Externalizing</rdfs:label>
        <rdfs:comment>Disinhibited Externalizing involves behaviors that are impulsive and lacking in social restraint or inhibition.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AntagonisticExternalizing">
        <rdfs:label>Antagonistic Externalizing</rdfs:label>
        <rdfs:comment>Antagonistic Externalizing describes behaviors that involve hostility and aggression towards others.</rdfs:comment>
    </owl:Class>

    <!-- This class encompasses all spectra belonging to the Superspectra category "General Psychopathology." -->
    <!-- The spectra include Somatoform, Internalizing, Thought Disorder, Detachment, Disinhibited Externalizing, and Antagonistic Externalizing. -->
    <rdf:Description rdf:about="http://purl.org/htp#GeneralPsychopathology">
        <rdf:type rdf:resource="http://purl.org/htp#Superspectra"/>
        <rdfs:label>General Psychopathology</rdfs:label>
        <rdfs:comment>General Psychopathology refers to the broad, overarching category of mental disorders and psychological symptoms that can manifest across different mental health conditions. It encompasses a wide range of psychological phenomena that are not specific to any single disorder but may contribute to or exacerbate multiple types of mental health issues. This category includes general symptoms and traits that are commonly observed in various psychiatric conditions, such as mood disturbances, anxiety, cognitive dysfunctions, and behavioral problems.</rdfs:comment>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#Somatoform"/>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#Internalizing"/>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#ThoughtDisorder"/>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#Detachment"/>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#DisinhibitedExternalizing"/>
        <htp:hasSpectra rdf:resource="http://purl.org/htp#AntagonisticExternalizing"/>    
        <htp:futureReleaseInfo>
            Indicates the potential for future inclusion of additional spectra and other related concepts as new developments and insights emerge in the field of general psychopathology.
            Allows for flexible expansion and refinement of the taxonomy to integrate new spectra, subfactors, and related elements to better capture evolving understanding and research findings.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- This spectra includes all components and disorders related to the "Somatoform" category. -->
    <!-- The components are Conversion, Somatization, Malaise, Head Pain, Gastrointestinal, and Cognitive. The associated disorders are Illness Anxiety Disorder and Somatic Symptom Disorder. -->
    <rdf:Description rdf:about="http://purl.org/htp#Somatoform">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Somatoform</rdfs:label>
        <rdfs:comment>The Somatoform label refers to a category of mental health disorders characterized by physical symptoms that cannot be fully explained by any underlying medical condition, highlighting the complex interaction between mind and body in mental health.</rdfs:comment>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Conversion"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Somatization"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Malaise"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#HeadPain"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Gastrointestinal"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Cognitive"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#IllnessAnxietyDisorder"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SomaticSymptomDisorder"/>
        <htp:futureReleaseInfo>
            This spectrum may include future expansions or modifications based on emerging research or changes in the understanding of somatoform disorders. 
            It is designed to adapt to new developments by allowing the addition of new related spectra or concepts as needed.
        </htp:futureReleaseInfo>
    </rdf:Description>    

    <!-- This spectra includes all subfactors and traits related to the "Internalizing" category. -->
    <!-- The subfactors are Sexual Problems, Eating Pathology, Fear, Distress, and Mania. The associated traits are Anxiousness, Emotional Lability, Hostility, Perseveration, Restricted Affectivity, Separation Insecurity, Submissiveness, Identity Problems, Negative Relationships, Fragility, Ineptitude, and Invulnerability. -->
    <rdf:Description rdf:about="http://purl.org/htp#Internalizing">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Internalizing</rdfs:label>
        <rdfs:comment>The Internalizing label refers to a category of mental health disorders characterized by inward-focused symptoms such as anxiety, depression, and withdrawal, reflecting emotional and cognitive distress.</rdfs:comment>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#SexualProblems"/>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#EatingPathology"/>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#Fear"/>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#Distress"/>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#Mania"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Anxiousness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#EmotionalLability"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Hostility"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Perseveration"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#RestrictedAffectivity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#SeparationInsecurity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Submissiveness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#IdentityProblems"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#NegativeRelationships"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Fragility"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Ineptitude"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Invulnerability"/>
        <htp:futureReleaseInfo>
            This spectrum can accommodate future additions or modifications based on emerging research or evolving understanding of internalizing disorders. 
            It is designed to integrate new related spectra or concepts as needed, reflecting the ongoing developments in the field.
        </htp:futureReleaseInfo>
    </rdf:Description>    

    <!-- This spectra category lists all subfactors, components, traits, and disorders associated with the "Thought Disorder" spectrum. -->
    <!-- The subfactors include Mania. The components are Psychotic, Disorganized, Inexpressivity, and Avolition. The traits are Eccentricity, Cognitive Perceptual Dysregulation, Unusual Beliefs and Experiences, and Fantasy Proneness. The disorders associated with this spectrum are Mood Disorders with Psychosis, Paranoid Personality Disorder (PD), Schizophrenia Spectrum, Schizoid Personality Disorder (PD), and Schizotypal Personality Disorder (PD). -->
    <rdf:Description rdf:about="http://purl.org/htp#ThoughtDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Thought Disorder</rdfs:label>
        <rdfs:comment>Thought Disorder refers to a category of mental health conditions characterized by disorganized thinking, impaired reality testing, and disturbances in the form or content of thought.</rdfs:comment>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#Mania"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Psychotic"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Disorganized"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Inexpressivity"/>
        <htp:hasComponent rdf:resource="http://purl.org/htp#Avolition"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Eccentricity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#CognitivePerceptualDysregulation"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#UnusualBeliefsAndExperiences"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#FantasyProneness"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#MoodDisordersWithPsychosis"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#ParanoidPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SchizophreniaSpectrum"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SchizoidPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SchizotypalPD"/>
        <htp:futureReleaseInfo>
            This spectrum allows for the integration of additional related spectra or concepts as new insights into thought disorders emerge. 
            It is structured to adapt to evolving understandings and classifications within the domain of cognitive and thought-related disturbances.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- This spectra category lists all traits and disorders associated with the "Detachment" spectrum. -->
    <!-- The traits include Anhedonia, Depressivity, Intimacy Avoidance, Suspiciousness, Withdrawal, Interpersonal Passivity, Disaffiliativeness, and Attention Seeking. The disorders associated with this spectrum are Avoidant Personality Disorder (PD), Histrionic Personality Disorder (PD), and Schizoid Personality Disorder (PD). -->
    <rdf:Description rdf:about="http://purl.org/htp#Detachment">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Detachment</rdfs:label>
        <rdfs:comment>Detachment refers to a psychological state characterized by emotional distance and disengagement from social interactions.</rdfs:comment>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Anhedonia"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Depressivity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#IntimacyAvoidance"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Suspiciousness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Withdrawal"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#InterpersonalPassivity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Disaffiliativeness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#AttentionSeeking"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#AvoidantPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#HistrionicPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SchizoidPD"/>
        <htp:futureReleaseInfo>
            This spectrum is designed to accommodate the inclusion of new related spectra or additional concepts as understanding of detachment and related psychological phenomena evolves. 
            It supports updates and extensions to reflect advances in research and shifts in diagnostic criteria.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- This spectra category lists all traits and subfactors associated with the "Disinhibited Externalizing" spectrum. -->
    <!-- The traits include Problematic Impulsivity, Irresponsibility, Theft, Distractibility, Risk-Taking, Rigid Perfectionism, Ruminative Deliberation, and Workaholism. The subfactors associated with this spectrum are Harmful Substance Abuse and Antisocial Behavior. -->
    <rdf:Description rdf:about="http://purl.org/htp#DisinhibitedExternalizing">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Disinhibited Externalizing</rdfs:label>
        <rdfs:comment>Disinhibited Externalizing involves behaviors that are impulsive and lacking in social restraint or inhibition.</rdfs:comment>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#HarmfulSubstanceAbuse"/>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#AntisocialBehavior"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#ProblematicImpulsivity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Irresponsibility"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Theft"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Distractibility"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#RiskTaking"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#RigidPerfectionism"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#RuminativeDeliberation"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Workaholism"/>
        <htp:futureReleaseInfo>
            This spectrum is designed to be adaptable to the addition of new spectra or related concepts as research on disinhibited externalizing behaviors evolves. 
            It allows for updates and expansions to incorporate emerging findings and shifts in diagnostic frameworks.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- This spectra category lists all traits and subfactors associated with the "Antagonistic Externalizing" spectrum. -->
    <!-- The traits include Attention Seeking, Callousness, Deceitfulness, Grandiosity, Manipulativeness, Rudeness, Egocentricity, Dominance, Flirtatiousness, and Timorousness. The subfactor associated with this spectrum is Antisocial Behavior. -->
    <!-- The disorders linked to this spectrum include Borderline Personality Disorder, Histrionic Personality Disorder, Narcissistic Personality Disorder, and Paranoid Personality Disorder. -->
    <rdf:Description rdf:about="http://purl.org/htp#AntagonisticExternalizing">
        <rdf:type rdf:resource="http://purl.org/htp#Spectra"/>
        <rdfs:label>Antagonistic Externalizing</rdfs:label>
        <rdfs:comment>Antagonistic Externalizing describes behaviors that involve hostility and aggression towards others.</rdfs:comment>
        <htp:hasSubfactor rdf:resource="http://purl.org/htp#AntisocialBehavior"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#AttentionSeeking"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Callousness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Deceitfulness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Grandiosity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Manipulativeness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Rudeness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Egocentricity"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Dominance"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Flirtatiousness"/>
        <htp:hasTrait rdf:resource="http://purl.org/htp#Timorousness"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#BorderlinePD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#HistrionicPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#NarcissisticPD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#ParanoidPD"/>
        <htp:futureReleaseInfo>
            This spectrum is designed to accommodate potential future modifications and expansions. 
            It supports the integration of new spectra or related concepts to reflect advancements in understanding antagonistic externalizing behaviors and their classification.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- These classes represent specific subfactors within the spectra. -->
    <!-- This categorization helps in understanding distinct aspects of psychological phenomena within each spectrum. -->
    <owl:Class rdf:about="http://purl.org/htp#SexualProblems">
        <rdfs:label>Sexual Problems</rdfs:label>
        <rdfs:comment>Sexual Problems encompass difficulties and dysfunctions related to sexual health and behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#EatingPathology">
        <rdfs:label>Eating Pathology</rdfs:label>
        <rdfs:comment>Eating Pathology includes disorders and maladaptive behaviors related to eating and body image.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Fear">
        <rdfs:label>Fear</rdfs:label>
        <rdfs:comment>Fear refers to an emotional response to perceived threats or danger.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Distress">
        <rdfs:label>Distress</rdfs:label>
        <rdfs:comment>Distress involves significant emotional suffering or discomfort.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Mania">
        <rdfs:label>Mania</rdfs:label>
        <rdfs:comment>Mania is characterized by elevated mood, increased activity, and impulsive behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#HarmfulSubstanceAbuse">
        <rdfs:label>Harmful Substance Abuse</rdfs:label>
        <rdfs:comment>Harmful Substance Abuse refers to the excessive use of substances that causes significant harm or impairment.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AntisocialBehavior">
    <rdfs:label>Antisocial Behavior</rdfs:label>
        <rdfs:comment>Antisocial Behavior involves actions that violate societal norms and the rights of others.</rdfs:comment>
    </owl:Class>

    <!-- This subfactor category represents difficulties and dysfunctions related to sexual health and behavior. -->
    <!-- It falls under the Internalizing spectrum and includes disorders like Arousal Difficulties, Low Desire, Orgasmic Function, and Sexual Pain. -->
    <rdf:Description rdf:about="http://purl.org/htp#SexualProblems">
        <rdf:type rdf:resource="http://purl.org/htp#Subfactor"/>
        <rdfs:label>Sexual Problems</rdfs:label>
        <rdfs:comment>Sexual Problems encompass difficulties and dysfunctions related to sexual health and behavior.</rdfs:comment>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#ArousalDifficulties"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#LowDesire"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#OrgasmicFunction"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#SexualPain"/>
        <htp:futureReleaseInfo>
            This subfactor is designed to be adaptable to future updates and expansions. 
            It allows for the incorporation of new elements or modifications to existing components as research advances and our understanding of psychological phenomena evolves.
        </htp:futureReleaseInfo>
    </rdf:Description>

    <!-- This subfactor category represents disorders and maladaptive behaviors related to eating and body image. -->
