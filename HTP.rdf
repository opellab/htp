        <htp:hasDisorder rdf:resource="http://purl.org/htp#BorderlinePD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#Dysthymia"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#GAD"/>
        <htp:hasDisorder rdf:resource="http://purl.org/htp#MDD"/>
