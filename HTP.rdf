    <!-- These classes represent specific components that contribute to various psychological phenomena or conditions. -->
    <!-- They include distinct aspects and manifestations relevant to understanding and diagnosing mental health issues. -->
    <owl:Class rdf:about="http://purl.org/htp#Conversion">
        <rdfs:label>Conversion</rdfs:label>
        <rdfs:comment>A component related to conversion disorders where psychological distress is expressed as physical symptoms.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Somatization">
        <rdfs:label>Somatization</rdfs:label>
        <rdfs:comment>A component involving the expression of psychological distress through physical symptoms.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Malaise">
        <rdfs:label>Malaise</rdfs:label>
        <rdfs:comment>A component related to general discomfort or unease.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#HeadPain">
        <rdfs:label>Head Pain</rdfs:label>
        <rdfs:comment>A component related to headaches or other types of head pain.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Gastrointestinal">
        <rdfs:label>Gastrointestinal</rdfs:label>
        <rdfs:comment>A component related to gastrointestinal symptoms and disorders.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Cognitive">
        <rdfs:label>Cognitive</rdfs:label>
        <rdfs:comment>A component related to cognitive symptoms and dysfunctions.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Psychotic">
        <rdfs:label>Psychotic</rdfs:label>
        <rdfs:comment>A component characterized by symptoms of psychosis.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Disorganized">
        <rdfs:label>Disorganized</rdfs:label>
        <rdfs:comment>A component characterized by disorganized thinking and behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Inexpressivity">
        <rdfs:label>Inexpressivity</rdfs:label>
        <rdfs:comment>A component characterized by a lack of emotional expression.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Avolition">
        <rdfs:label>Avolition</rdfs:label>
        <rdfs:comment>A component characterized by a lack of motivation to initiate and sustain purposeful activities.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://purl.org/htp#InteractiveAnxiety">
        <rdfs:label>Interactive Anxiety</rdfs:label>
        <rdfs:comment>Anxiety experienced in social interactions and group settings.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#PerformanceAnxiety">
        <rdfs:label>Performance Anxiety</rdfs:label>
        <rdfs:comment>Anxiety related to performing tasks or activities, often in public or under observation.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#PublicPlaces">
        <rdfs:label>Public Places</rdfs:label>
        <rdfs:comment>Fear or anxiety associated with being in public spaces.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#EnclosedSpaces">
        <rdfs:label>Enclosed Spaces</rdfs:label>
        <rdfs:comment>Fear or anxiety related to confined or small spaces.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AnimalPhobia">
        <rdfs:label>Animal Phobia</rdfs:label>
        <rdfs:comment>Fear or anxiety related to animals.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#SituationalPhobia">
        <rdfs:label>Situational Phobia</rdfs:label>
        <rdfs:comment>Fear or anxiety related to specific situations or environments.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Blood-injection-injury">
        <rdfs:label>Blood-Injection-Injury Phobia</rdfs:label>
        <rdfs:comment>Fear or anxiety related to blood, injections, or injuries.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#PhysiologicalPanic">
        <rdfs:label>Physiological Panic</rdfs:label>
        <rdfs:comment>Panic attacks characterized by physiological symptoms such as rapid heartbeat and sweating.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Cleaning">
        <rdfs:label>Cleaning</rdfs:label>
        <rdfs:comment>Compulsive behaviors related to cleaning or hygiene.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Rituals">
        <rdfs:label>Rituals</rdfs:label>
        <rdfs:comment>Compulsive rituals or behaviors performed in a specific manner.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Checking">
        <rdfs:label>Checking</rdfs:label>
        <rdfs:comment>Compulsive checking behaviors, such as repeatedly checking locks or appliances.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Dysphoria">
        <rdfs:label>Dysphoria</rdfs:label>
        <rdfs:comment>A state of generalized unhappiness or dissatisfaction.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Lassitude">
        <rdfs:label>Lassitude</rdfs:label>
        <rdfs:comment>A state of physical or mental weariness and lack of energy.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Anhedonia">
        <rdfs:label>Anhedonia</rdfs:label>
        <rdfs:comment>The inability to feel pleasure or interest in activities that are normally enjoyable.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Insomnia">
        <rdfs:label>Insomnia</rdfs:label>
        <rdfs:comment>Difficulty falling asleep or staying asleep, or waking up too early and not being able to go back to sleep.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Suicidality">
        <rdfs:label>Suicidality</rdfs:label>
        <rdfs:comment>The presence of thoughts or behaviors related to suicide.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Agitation">
        <rdfs:label>Agitation</rdfs:label>
        <rdfs:comment>State of being overly excited or disturbed, often resulting in restlessness or erratic behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Retardation">
        <rdfs:label>Retardation</rdfs:label>
        <rdfs:comment>Slowed cognitive and physical processes, including thinking, speaking, and moving.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AppetiteLoss">
        <rdfs:label>Appetite Loss</rdfs:label>
        <rdfs:comment>Reduction or loss of desire to eat, which can lead to weight loss.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AppetiteGain">
        <rdfs:label>Appetite Gain</rdfs:label>
        <rdfs:comment>Increased desire to eat, which can lead to weight gain.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Wellbeing">
        <rdfs:label>Wellbeing</rdfs:label>
        <rdfs:comment>A positive state of mental and physical health, encompassing feelings of happiness and contentment.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#GADSymptoms">
        <rdfs:label>GAD Symptoms</rdfs:label>
        <rdfs:comment>Symptoms associated with Generalized Anxiety Disorder, including persistent and excessive worry about various aspects of life.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#ReExperiencing">
        <rdfs:label>Re-Experiencing</rdfs:label>
        <rdfs:comment>Re-living traumatic events through flashbacks, intrusive memories, or nightmares.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Avoidance">
        <rdfs:label>Avoidance</rdfs:label>
        <rdfs:comment>Efforts to avoid reminders or situations that are associated with traumatic experiences or stress.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Hyperarousal">
        <rdfs:label>Hyperarousal</rdfs:label>
        <rdfs:comment>Increased physiological arousal, including symptoms such as hypervigilance, irritability, and difficulty sleeping.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Numbing">
        <rdfs:label>Numbing</rdfs:label>
        <rdfs:comment>Emotional numbness or detachment from feelings as a response to overwhelming stress or trauma.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Dissociation">
        <rdfs:label>Dissociation</rdfs:label>
        <rdfs:comment>A mental process used to cope with stress or trauma, leading to a sense of detachment from one's surroundings or self.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Irritability">
        <rdfs:label>Irritability</rdfs:label>
        <rdfs:comment>State of being easily annoyed or angered, often with a low tolerance for frustration.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#PureObsessions">
        <rdfs:label>Pure Obsessions</rdfs:label>
        <rdfs:comment>Recurrent and persistent intrusive thoughts that cause significant distress, without accompanying compulsive behaviors.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#EuphoricActivation">
        <rdfs:label>Euphoric Activation</rdfs:label>
        <rdfs:comment>A state of heightened mood and increased energy, often associated with mania or hypomania.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#HyperactiveCognition">
        <rdfs:label>Hyperactive Cognition</rdfs:label>
        <rdfs:comment>Increased cognitive activity, often characterized by racing thoughts and distractibility.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RecklessOverConfidence">
        <rdfs:label>Reckless Over-Confidence</rdfs:label>
        <rdfs:comment>Excessive self-confidence leading to risky or impulsive behavior, often seen in manic episodes.</rdfs:comment>
    </owl:Class>
  
    <owl:Class rdf:about="http://purl.org/htp#AlcoholUse">
        <rdfs:label>Alcohol Use</rdfs:label>
        <rdfs:comment>The consumption of alcoholic beverages, which may lead to problematic use.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AlcoholProblems">
        <rdfs:label>Alcohol Problems</rdfs:label>
        <rdfs:comment>Issues related to the use of alcohol, including dependence and abuse.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#MarijuanaUse">
        <rdfs:label>Marijuana Use</rdfs:label>
        <rdfs:comment>The consumption of marijuana, which may lead to problematic use.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#MarijuanaProblems">
        <rdfs:label>Marijuana Problems</rdfs:label>
        <rdfs:comment>Issues related to the use of marijuana, including dependence and abuse.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#DrugUse">
        <rdfs:label>Drug Use</rdfs:label>
        <rdfs:comment>The consumption of illicit or prescription drugs, which may lead to problematic use.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#DrugProblems">
        <rdfs:label>Drug Problems</rdfs:label>
        <rdfs:comment>Issues related to the use of drugs, including dependence and abuse.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#PhysicalAggression">
        <rdfs:label>Physical Aggression</rdfs:label>
        <rdfs:comment>Aggression involving physical acts of violence towards others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#DestructiveAggression">
        <rdfs:label>Destructive Aggression</rdfs:label>
        <rdfs:comment>Aggression that results in damage to property or the environment.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RelationalAggression">
        <rdfs:label>Relational Aggression</rdfs:label>
        <rdfs:comment>Aggression that harms others through manipulation of social relationships.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Fraud">
        <rdfs:label>Fraud</rdfs:label>
        <rdfs:comment>Deceptive behavior intended to secure unfair or unlawful gain.</rdfs:comment>
    </owl:Class>

    <!-- These classes represent specific traits that characterize individual differences in psychological functioning. -->
    <!-- They include attributes and tendencies that influence behavior and emotional responses. -->
    <owl:Class rdf:about="http://purl.org/htp#Anxiousness">
        <rdfs:label>Anxiousness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to experience anxiety or nervousness.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#EmotionalLability">
        <rdfs:label>Emotional Lability</rdfs:label>
        <rdfs:comment>A trait characterized by rapid and intense emotional changes.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Hostility">
        <rdfs:label>Hostility</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency toward aggression or antagonism.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Perseveration">
        <rdfs:label>Perseveration</rdfs:label>
        <rdfs:comment>A trait characterized by the repetition of a particular response despite the absence or cessation of a stimulus.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RestrictedAffectivity">
        <rdfs:label>Restricted Affectivity</rdfs:label>
        <rdfs:comment>A trait characterized by limited or constricted emotional expression.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#SeparationInsecurity">
        <rdfs:label>Separation Insecurity</rdfs:label>
        <rdfs:comment>A trait characterized by anxiety and insecurity in response to separation from significant others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Submissiveness">
        <rdfs:label>Submissiveness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to submit to others and avoid asserting oneself.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#IdentityProblems">
        <rdfs:label>Identity Problems</rdfs:label>
        <rdfs:comment>A trait characterized by difficulties in forming or maintaining a stable sense of self.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#NegativeRelationships">
        <rdfs:label>Negative Relationships</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to engage in relationships that are harmful or unsatisfactory.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Fragility">
        <rdfs:label>Fragility</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of resilience and a tendency to be easily hurt or affected by stress.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Ineptitude">
        <rdfs:label>Ineptitude</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of skill or competence.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Invulnerability">
        <rdfs:label>Invulnerability</rdfs:label>
        <rdfs:comment>A trait characterized by a perception of being immune to harm or negative effects.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Eccentricity">
        <rdfs:label>Eccentricity</rdfs:label>
        <rdfs:comment>A trait characterized by unconventional or peculiar behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#CognitivePerceptualDysregulation">
        <rdfs:label>Cognitive Perceptual Dysregulation</rdfs:label>
        <rdfs:comment>A trait characterized by disturbances in cognition and perception.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#UnusualBeliefsAndExperiences">
        <rdfs:label>Unusual Beliefs and Experiences</rdfs:label>
        <rdfs:comment>A trait characterized by the presence of atypical beliefs and experiences.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#FantasyProneness">
        <rdfs:label>Fantasy Proneness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to engage in vivid and complex fantasies.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Anhedonia">
        <rdfs:label>Anhedonia</rdfs:label>
        <rdfs:comment>A trait characterized by the loss of interest or pleasure in activities that are usually enjoyable.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Depressivity">
        <rdfs:label>Depressivity</rdfs:label>
        <rdfs:comment>A trait characterized by feelings of sadness and hopelessness.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#IntimacyAvoidance">
        <rdfs:label>Intimacy Avoidance</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to avoid close relationships and emotional intimacy.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Suspiciousness">
        <rdfs:label>Suspiciousness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to be distrustful and suspicious of others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Withdrawal">
        <rdfs:label>Withdrawal</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to retreat from social interactions and avoid engagement with others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#InterpersonalPassivity">
        <rdfs:label>Interpersonal Passivity</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of initiative or assertiveness in social interactions.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Disaffiliativeness">
        <rdfs:label>Disaffiliativeness</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of interest in forming or maintaining social bonds.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AttentionSeeking">
        <rdfs:label>Attention Seeking</rdfs:label>
        <rdfs:comment>A trait characterized by behaviors that are intended to attract attention or gain approval from others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#ProblematicImpulsivity">
        <rdfs:label>Problematic Impulsivity</rdfs:label>
        <rdfs:comment>A trait characterized by difficulty in controlling impulsive behaviors, often leading to negative consequences.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Irresponsibility">
        <rdfs:label>Irresponsibility</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of regard for the consequences of one's actions, often leading to reckless behavior.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Theft">
        <rdfs:label>Theft</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to steal or engage in dishonest acquisition of property.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Distractibility">
        <rdfs:label>Distractibility</rdfs:label>
        <rdfs:comment>A trait characterized by an inability to maintain focus, leading to frequent shifts in attention.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RiskTaking">
        <rdfs:label>Risk Taking</rdfs:label>
        <rdfs:comment>A trait characterized by engaging in behaviors that involve significant risk, often without regard for potential negative outcomes.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RigidPerfectionism">
        <rdfs:label>Rigid Perfectionism</rdfs:label>
        <rdfs:comment>A trait characterized by an excessive and inflexible striving for perfection and high standards, often leading to self-criticism and stress.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#RuminativeDeliberation">
        <rdfs:label>Ruminative Deliberation</rdfs:label>
        <rdfs:comment>A trait characterized by excessive and repetitive thinking about potential decisions and their outcomes, often leading to indecision and stress.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Workaholism">
        <rdfs:label>Workaholism</rdfs:label>
        <rdfs:comment>A trait characterized by an excessive and compulsive commitment to work, often at the expense of personal well-being and relationships.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#AttentionSeeking">
        <rdfs:label>Attention Seeking</rdfs:label>
        <rdfs:comment>A trait characterized by a strong desire to attract and hold the attention of others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Callousness">
        <rdfs:label>Callousness</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of empathy and a tendency to be emotionally cold or indifferent to others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Deceitfulness">
        <rdfs:label>Deceitfulness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to lie, cheat, or deceive others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Grandiosity">
        <rdfs:label>Grandiosity</rdfs:label>
        <rdfs:comment>A trait characterized by an exaggerated sense of one's own importance, abilities, or achievements.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Manipulativeness">
        <rdfs:label>Manipulativeness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to control or influence others for personal gain, often in a devious manner.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Rudeness">
        <rdfs:label>Rudeness</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of manners or respect in social interactions.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Egocentricity">
        <rdfs:label>Egocentricity</rdfs:label>
        <rdfs:comment>A trait characterized by an excessive focus on oneself and one's own needs and desires.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Dominance">
        <rdfs:label>Dominance</rdfs:label>
        <rdfs:comment>A trait characterized by a desire to control or lead others, often displaying assertiveness and power.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Flirtatiousness">
        <rdfs:label>Flirtatiousness</rdfs:label>
        <rdfs:comment>A trait characterized by behavior that seeks to attract romantic or sexual interest from others through playful or provocative means.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Timorousness">
        <rdfs:label>Timorousness</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to be fearful or lacking in confidence, often leading to shyness or hesitation.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#ImpatientUrgency">
        <rdfs:label>Impatient Urgency</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of patience and a tendency to act impulsively in urgent situations.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#LowPlanfulControl">
        <rdfs:label>Low Planful Control</rdfs:label>
        <rdfs:comment>A trait indicating poor ability to plan and execute tasks effectively.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#LowDependability">
        <rdfs:label>Low Dependability</rdfs:label>
        <rdfs:comment>A trait characterized by a lack of reliability and consistency in fulfilling obligations.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Alienation">
        <rdfs:label>Alienation</rdfs:label>
        <rdfs:comment>A trait reflecting a sense of estrangement or detachment from others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#BoredomProneness">
        <rdfs:label>Boredom Proneness</rdfs:label>
        <rdfs:comment>A trait indicating a tendency to experience boredom frequently and easily.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#BlameExternalization">
        <rdfs:label>Blame Externalization</rdfs:label>
        <rdfs:comment>A trait characterized by a tendency to attribute failures or problems to external factors rather than internal ones.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#LowHonesty">
        <rdfs:label>Low Honesty</rdfs:label>
        <rdfs:comment>A trait marked by a tendency to be deceitful or untruthful.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#Rebelliousness">
        <rdfs:label>Rebelliousness</rdfs:label>
        <rdfs:comment>A trait characterized by resistance to authority and norms.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#LowEmpathy">
        <rdfs:label>Low Empathy</rdfs:label>
        <rdfs:comment>A trait indicating a reduced ability to understand and share the feelings of others.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/htp#ExcitementSeeking">
        <rdfs:label>Excitement Seeking</rdfs:label>
        <rdfs:comment>A trait characterized by a desire for new, thrilling, or risky experiences.</rdfs:comment>
    </owl:Class>

    <!-- These classes represent specific disorders that involve significant disturbances in mental health and functioning. -->
    <!-- They encompass various diagnostic conditions that impact thoughts, emotions, and behavior. -->
    <rdf:Description rdf:about="http://purl.org/htp#IllnessAnxietyDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Illness Anxiety Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by excessive worry about having a serious illness, despite having no or minimal symptoms.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SomaticSymptomDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Somatic Symptom Disorder</rdfs:label>
        <rdfs:comment>A disorder involving one or more somatic symptoms that are distressing or result in significant disruption in daily life, along with excessive thoughts, feelings, or behaviors related to the symptoms.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#MoodDisordersWithPsychosis">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Mood Disorders with Psychosis</rdfs:label>
        <rdfs:comment>A disorder characterized by mood disturbances accompanied by psychotic symptoms.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#ParanoidPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Paranoid Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by pervasive distrust and suspicion of others.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SchizophreniaSpectrum">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Schizophrenia Spectrum</rdfs:label>
        <rdfs:comment>A spectrum of disorders characterized by disturbances in thought, perception, and behavior.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SchizotypalPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Schizotypal Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by acute discomfort in close relationships and eccentric behavior.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#AvoidantPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Avoidant Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by social inhibition, feelings of inadequacy, and hypersensitivity to negative evaluation.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#HistrionicPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Histrionic Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by excessive emotionality and attention-seeking behavior.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SchizoidPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Schizoid Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by a pervasive pattern of detachment from social relationships and a restricted range of emotional expression.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#NarcissisticPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Narcissistic Personality Disorder</rdfs:label>
        <rdfs:comment>A personality disorder characterized by a pervasive pattern of grandiosity, need for admiration, and a lack of empathy.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#ArousalDifficulties">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Arousal Difficulties</rdfs:label>
        <rdfs:comment>A disorder characterized by difficulties in achieving or maintaining sexual arousal.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#LowDesire">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Low Desire</rdfs:label>
        <rdfs:comment>A disorder characterized by a significantly reduced interest in sexual activity.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#OrgasmicFunction">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Orgasmic Function</rdfs:label>
        <rdfs:comment>A disorder characterized by difficulties in achieving orgasm.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SexualPain">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Sexual Pain</rdfs:label>
        <rdfs:comment>A disorder characterized by pain during sexual activity.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#AnorexiaNervosa">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Anorexia Nervosa</rdfs:label>
        <rdfs:comment>A disorder characterized by an intense fear of gaining weight and a distorted body image, leading to restricted food intake and excessive weight loss.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#BingeEatingDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Binge Eating Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by frequent episodes of consuming large quantities of food, often quickly and to the point of discomfort, accompanied by a sense of loss of control during the binge and feelings of shame or distress.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#BulimiaNervosa">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Bulimia Nervosa</rdfs:label>
        <rdfs:comment>A disorder characterized by binge eating followed by purging behaviors, such as vomiting or excessive use of laxatives, to prevent weight gain.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#Agoraphobia">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Agoraphobia</rdfs:label>
        <rdfs:comment>A disorder characterized by intense fear of being in situations where escape might be difficult.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#OCD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Obsessive-Compulsive Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by unwanted repetitive thoughts (obsessions) and/or actions (compulsions).</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#PanicDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Panic Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by recurrent and unexpected panic attacks.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SAD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Social Anxiety Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by an intense fear of social situations.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SocialPhobia">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Social Phobia</rdfs:label>
        <rdfs:comment>Excessive and persistent fear of social interactions and situations.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SpecificPhobia">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Specific Phobia</rdfs:label>
        <rdfs:comment>A disorder characterized by intense, irrational fear of specific objects or situations.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#BorderlinePD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Borderline Personality Disorder</rdfs:label>
        <rdfs:comment>A personality disorder characterized by unstable moods, behavior, and relationships.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#Dysthymia">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Dysthymia</rdfs:label>
        <rdfs:comment>Chronic low-grade depression lasting for at least two years.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#GAD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Generalized Anxiety Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by excessive, uncontrollable worry about various aspects of life.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#MDD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Major Depressive Disorder</rdfs:label>
        <rdfs:comment>A mood disorder characterized by persistent feelings of sadness and loss of interest or pleasure in activities.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#PTSD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Post-Traumatic Stress Disorder</rdfs:label>
        <rdfs:comment>A mental health condition triggered by experiencing or witnessing a traumatic event, leading to flashbacks, nightmares, and severe anxiety.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#BipolarI">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Bipolar I Disorder</rdfs:label>
        <rdfs:comment>A mood disorder characterized by at least one manic episode, often accompanied by depressive episodes.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#BipolarII">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Bipolar II Disorder</rdfs:label>
        <rdfs:comment>A mood disorder characterized by at least one hypomanic episode and one major depressive episode, but no full manic episodes.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#SubstanceRelatedDisorders">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Substance-Related Disorders</rdfs:label>
        <rdfs:comment>Disorders characterized by problematic use of substances, including alcohol, marijuana, and other drugs.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#AntisocialPD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Antisocial Personality Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by a persistent pattern of disregard for, and violation of, the rights of others.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#ConductDisorder">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Conduct Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by a repetitive and persistent pattern of behavior that violates societal norms and the rights of others.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#IED">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Intermittent Explosive Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by recurrent outbursts of anger or violence that are disproportionate to the situation.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#ODD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Oppositional Defiant Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by a pattern of angry or irritable mood, argumentative or defiant behavior, or vindictiveness.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="http://purl.org/htp#ADHD">
        <rdf:type rdf:resource="http://purl.org/htp#Disorder"/>
        <rdfs:label>Attention-Deficit/Hyperactivity Disorder</rdfs:label>
        <rdfs:comment>A disorder characterized by a persistent pattern of inattention and/or hyperactivity-impulsivity that interferes with functioning or development.</rdfs:comment>
    </rdf:Description>

</rdf:RDF>